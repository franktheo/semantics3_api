<div class="container">
   <div class="rows"> 
      <div class="col-xs-12">
         <% case current_user.role %>
            <% when "admin" %>
               <%= render partial: "admin" %>
            <% when "guest" %>
               <%= render partial: "guest" %>
         <% end %>
               
         <%= link_to 'Log out', 'logout', class: 'btn btn-primary btn-xs' %><br>

      </div>
   </div>
</div>
